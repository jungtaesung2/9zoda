{
  "name": "petsitter",
  "version": "1.0.0",
  "main": "./src/app.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write **/*.js",
    "dev": "nodemon src/app.js",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "test:esm": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:silent": "cross-env NODE_ENV=test jest --silent --forceExit",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --forceExit",
    "test:unit": "cross-env NODE_ENV=test jest __tests__/unit --forceExit"
  },
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  }
}
